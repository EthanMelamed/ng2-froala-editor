{
  "name": "ng2-froala-editor",
  "version": "1.0.8",
  "description": "Froala Editor for Angular2",
  "repository": {
    "type": "git",
    "url": "https://github.com/ericleethrinacia/ng2-froala-editor"
  },
  "scripts": {
    "typings-install": "typings install",
    "tsc": "tsc",
    "tsc:w": "tsc -w",
    "typings": "typings",
    "play": "webpack-dev-server --inline --port 8085",
    "build-cjs": "tsc -p tsconfig.common.json",
    "build-system": "tsc -p tsconfig.system.json",
    "build": "rimraf dist && npm run build-cjs && npm run build-system && webpack --progress --colors --profile --bail --config webpack.prod.js && gulp build",
    "test": "karma start config/karma.conf.js"
  },
  "license": "ISC",
  "dependencies": {
    "jquery": "^3.1.0",
    "froala-editor": "^2.3.3"
  },
  "devDependencies": {
    "@angular/common": "2.0.0-rc.4",
    "@angular/compiler": "2.0.0-rc.4",
    "@angular/core": "2.0.0-rc.4",
    "@angular/forms": "0.2.0",
    "@angular/http": "2.0.0-rc.4",
    "@angular/platform-browser": "2.0.0-rc.4",
    "@angular/platform-browser-dynamic": "2.0.0-rc.4",
    "@angular/router": "3.0.0-beta.1",
    "@angular/router-deprecated": "2.0.0-rc.2",
    "@angular/upgrade": "2.0.0-rc.4",
    "angular2-in-memory-web-api": "0.0.14",
    "angular2-template-loader": "^0.4.0",
    "concurrently": "^2.0.0",
    "core-js": "^2.4.0",
    "extract-text-webpack-plugin": "^1.0.1",
    "gulp": "^3.9.1",
    "html-loader": "^0.4.3",
    "html-webpack-plugin": "^2.22.0",
    "jasmine-core": "^2.4.1",
    "karma": "^1.1.1",
    "karma-jasmine": "^1.0.2",
    "karma-phantomjs-launcher": "^1.0.1",
    "karma-sourcemap-loader": "^0.3.7",
    "karma-webpack": "^1.7.0",
    "lite-server": "^2.2.0",
    "phantomjs-prebuilt": "^2.1.7",
    "reflect-metadata": "^0.1.3",
    "rxjs": "5.0.0-beta.6",
    "systemjs": "0.19.27",
    "ts-loader": "^0.8.2",
    "typescript": "^1.8.10",
    "typings": "^1.0.4",
    "webpack": "^1.13.1",
    "webpack-dev-server": "^1.14.1",
    "webpack-merge": "^0.14.0",
    "zone.js": "^0.6.12"
  }
